	"Print to boxshadow": {
		"prefix": ["box-shadow","boxshadow"],
		"body": [
			"box-shadow: 0 0 10px 0 rgb(40 40 40 / 30%);"
		],
		"description": "offset-x | offset-y | blur-radius | spread-radius | color"
	},
	"Print to textshadow": {
		"prefix": ["text-shadow", "textshadow"],
		"body": [
			"text-shadow: 1px 1px 2px \\$bgBlack, 0 0 1em \\$bgBlack, 0 0 0.2em \\$bgBlack;"
		],
		"description": "offset-x | offset-y | blur-radius | color"
	},
	"Print to lineclamp": {
		"prefix": "lineclamp",
		"body": [
			"display: -webkit-box !important;",
			"-webkit-line-clamp: 2;",
			"-webkit-box-orient: vertical;",
			"overflow: hidden;"
		],
		"description": "lineclamp"
	},
	"media min width": {
		"prefix": ["mediamin", "minmedia"],
		"body": [
			"@media (min-width: \\$${1|mdScreen,xxlScreen,xlScreen,lgScreen,smScreen|}){",
			"\t$2",
			"}"
		],
		"description": "media screen  \n $xxlScreen: 1400px; \n $xlScreen: 1170px; \n $lgScreen: 992px; \n $mdScreen: 768px; \n smScreen: 576px;"
	},
	"media max": {
		"prefix": ["mediamax","maxmedia"],
		"body": [
			"@media (max-width: \\$${1|mdScreen,xxlScreen,xlScreen,lgScreen,smScreen|} - 1){",
			"\t$2",
			"}"
		],
		"description": "media screen  \n $xxlScreen: 1400px; \n $xlScreen: 1170px; \n $lgScreen: 992px; \n $mdScreen: 768px; \n smScreen: 576px;"
	},
	"keyframes ": {
		"prefix": "keyframes",
